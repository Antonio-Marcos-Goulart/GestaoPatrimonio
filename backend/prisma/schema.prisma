datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Categoria {
  id          Int          @id @default(autoincrement())
  nome        String
  patrimonios Patrimonio[]
}

model Patrimonio {
  id        Int      @id @default(autoincrement())
  nome      String
  valor     Decimal
  createdAt DateTime @default(now())

  categoriaId Int
  categoria   Categoria @relation(fields: [categoriaId], references: [id])
}
