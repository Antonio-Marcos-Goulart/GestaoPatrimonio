datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Categoria {
  id          Int          @id @default(autoincrement())
  nome        String
  patrimonios Patrimonio[]
}

model Patrimonio {
  id        Int      @id @default(autoincrement())
  nome      String
  valor     Decimal
  createdAt DateTime @default(now())
}
